{
  "expo": {
    "name": "Sessiz Ortam",
    "slug": "sessiz-ortam-bulucu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sessizortam",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arda.sessizortam",
      "config": {
        "googleMapsApiKey": "AIzaSyD2WkVyjwi9s0oBflGzw8oF3FPJO7K_3_A"
      }
    },
    "android": {
      "package": "com.arda.sessizortam",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "INTERNET"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD2WkVyjwi9s0oBflGzw8oF3FPJO7K_3_A"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Sessiz ortamları bulmak için konumunuza ihtiyacımız var."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "61872f08-8cc9-40bb-82ed-49450ff53a0f"
      }
    }
  }
}